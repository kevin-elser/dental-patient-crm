generator client {
  provider = "prisma-client-js"
}

generator patientColors {
  provider = "ts-node --transpile-only ./prisma/color-generator.ts"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model account {
  AccountNum         BigInt @id @default(autoincrement())
  Description        String @db.VarChar(255)
  AcctType           Int    @db.UnsignedTinyInt
  BankNumber         String @db.VarChar(255)
  Inactive           Int    @db.UnsignedTinyInt
  AccountColor       Int
  IsRetainedEarnings Int    @db.TinyInt
}

model accountingautopay {
  AccountingAutoPayNum BigInt  @id @default(autoincrement())
  PayType              BigInt
  PickList             String? @default("") @db.VarChar(255)
}

model activeinstance {
  ActiveInstanceNum  BigInt   @id @default(autoincrement())
  ComputerNum        BigInt
  UserNum            BigInt
  ProcessId          BigInt
  DateTimeLastActive DateTime @default(dbgenerated("('0001-01-01 00:00:00')")) @db.DateTime(0)
  DateTRecorded      DateTime @default(dbgenerated("('0001-01-01 00:00:00')")) @db.DateTime(0)
  ConnectionType     Int      @db.TinyInt

  @@index([ComputerNum], map: "ComputerNum")
  @@index([ProcessId], map: "ProcessId")
  @@index([UserNum], map: "UserNum")
}

model adjustment {
  AdjNum          BigInt   @id @default(autoincrement())
  AdjDate         DateTime @default(dbgenerated("('0001-01-01')")) @db.Date
  AdjAmt          Float    @default(0)
  PatNum          BigInt
  AdjType         BigInt
  ProvNum         BigInt
  AdjNote         String?  @db.Text
  ProcDate        DateTime @default(dbgenerated("('0001-01-01')")) @db.Date
  ProcNum         BigInt
  DateEntry       DateTime @default(dbgenerated("('0001-01-01')")) @db.Date
  ClinicNum       BigInt
  StatementNum    BigInt
  SecUserNumEntry BigInt
  SecDateTEdit    DateTime @default(now()) @db.Timestamp(0)
  TaxTransID      BigInt

  @@index([AdjDate, PatNum], map: "AdjDatePN")
  @@index([ClinicNum], map: "ClinicNum")
  @@index([ProcNum], map: "ProcNum")
  @@index([SecDateTEdit, PatNum], map: "SecDateTEditPN")
  @@index([SecUserNumEntry], map: "SecUserNumEntry")
  @@index([StatementNum], map: "StatementNum")
  @@index([TaxTransID], map: "TaxTransID")
  @@index([ProcNum, AdjAmt], map: "indexPNAmt")
  @@index([PatNum], map: "indexPatNum")
  @@index([ProvNum], map: "indexProvNum")
}

model alertcategory {
  AlertCategoryNum BigInt @id @default(autoincrement())
  IsHQCategory     Int    @db.TinyInt
  InternalName     String @db.VarChar(255)
  Description      String @db.VarChar(255)
}

model alertcategorylink {
  AlertCategoryLinkNum BigInt @id @default(autoincrement())
  AlertCategoryNum     BigInt
  AlertType            Int    @db.TinyInt

  @@index([AlertCategoryNum], map: "AlertCategoryNum")
}

model alertitem {
  AlertItemNum  BigInt   @id @default(autoincrement())
  ClinicNum     BigInt
  Description   String   @db.VarChar(2000)
  Type          Int      @db.TinyInt
  Severity      Int      @db.TinyInt
  Actions       Int      @db.TinyInt
  FormToOpen    Int      @db.TinyInt
  FKey          BigInt
  ItemValue     String   @db.VarChar(4000)
  UserNum       BigInt
  SecDateTEntry DateTime @default(dbgenerated("('0001-01-01 00:00:00')")) @db.DateTime(0)

  @@index([ClinicNum], map: "ClinicNum")
  @@index([FKey], map: "FKey")
  @@index([UserNum], map: "UserNum")
}

model alertread {
  AlertReadNum BigInt @id @default(autoincrement())
  AlertItemNum BigInt
  UserNum      BigInt

  @@index([AlertItemNum], map: "AlertItemNum")
  @@index([UserNum], map: "UserNum")
}

model alertsub {
  AlertSubNum      BigInt @id @default(autoincrement())
  UserNum          BigInt
  ClinicNum        BigInt
  Type             Int    @db.TinyInt
  AlertCategoryNum BigInt

  @@index([AlertCategoryNum], map: "AlertCategoryNum")
  @@index([ClinicNum], map: "ClinicNum")
  @@index([UserNum], map: "UserNum")
}

model allergy {
  AllergyNum          BigInt   @id @default(autoincrement())
  AllergyDefNum       BigInt
  PatNum              BigInt
