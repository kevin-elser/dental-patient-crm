  @@index([MedicationNum], map: "MedicationNum")
}

model apikey {
  APIKeyNum  BigInt @id @default(autoincrement())
  CustApiKey String @db.VarChar(255)
  DevName    String @db.VarChar(255)
}

model apisubscription {
  ApiSubscriptionNum BigInt   @id @default(autoincrement())
  EndPointUrl        String   @db.VarChar(255)
  Workstation        String   @db.VarChar(255)
  CustomerKey        String   @db.VarChar(255)
  WatchTable         String   @db.VarChar(255)
  PollingSeconds     Int
  UiEventType        String   @db.VarChar(255)
  DateTimeStart      DateTime @default(dbgenerated("('0001-01-01 00:00:00')")) @db.DateTime(0)
  DateTimeStop       DateTime @default(dbgenerated("('0001-01-01 00:00:00')")) @db.DateTime(0)
  Note               String   @db.VarChar(255)

  @@index([PollingSeconds], map: "PollingSeconds")
}

model appointment {
  AptNum                BigInt   @id @default(autoincrement())
  PatNum                BigInt
  AptStatus             Int      @default(0) @db.UnsignedTinyInt
  Pattern               String?  @default("") @db.VarChar(255)
  Confirmed             BigInt
  TimeLocked            Boolean
  Op                    BigInt
  Note                  String?  @db.Text
  ProvNum               BigInt
  ProvHyg               BigInt
  AptDateTime           DateTime @default(dbgenerated("('0001-01-01 00:00:00')")) @db.DateTime(0)
  NextAptNum            BigInt
  UnschedStatus         BigInt
  IsNewPatient          Boolean  @default(false)
  ProcDescript          String?  @default("") @db.VarChar(255)
  Assistant             BigInt
  ClinicNum             BigInt
  IsHygiene             Int      @db.UnsignedTinyInt
  DateTStamp            DateTime @default(now()) @db.Timestamp(0)
  DateTimeArrived       DateTime @db.DateTime(0)
  DateTimeSeated        DateTime @db.DateTime(0)
  DateTimeDismissed     DateTime @db.DateTime(0)
  InsPlan1              BigInt
  InsPlan2              BigInt
  DateTimeAskedToArrive DateTime @default(dbgenerated("('0001-01-01 00:00:00')")) @db.DateTime(0)
  ProcsColored          String   @db.Text
  ColorOverride         Int
  AppointmentTypeNum    BigInt
  SecUserNumEntry       BigInt
  SecDateTEntry         DateTime @default(dbgenerated("('0001-01-01 00:00:00')")) @db.DateTime(0)
  Priority              Int      @db.TinyInt
  ProvBarText           String   @db.VarChar(60)
  PatternSecondary      String   @db.VarChar(255)
  SecurityHash          String   @db.VarChar(255)
  ItemOrderPlanned      Int

  @@index([AppointmentTypeNum], map: "AppointmentTypeNum")
  @@index([ClinicNum], map: "ClinicNum")
  @@index([DateTimeArrived], map: "DateTimeArrived")
  @@index([InsPlan1], map: "InsPlan1")
  @@index([InsPlan2], map: "InsPlan2")
  @@index([Op], map: "Op")
  @@index([Priority], map: "Priority")
  @@index([SecUserNumEntry], map: "SecUserNumEntry")
  @@index([AptStatus, AptDateTime, ClinicNum], map: "StatusDate")
  @@index([UnschedStatus], map: "UnschedStatus")
  @@index([AptDateTime], map: "indexAptDateTime")
  @@index([NextAptNum], map: "indexNextAptNum")
  @@index([PatNum], map: "indexPatNum")
  @@index([ProvHyg], map: "indexProvHyg")
  @@index([ProvNum], map: "indexProvNum")
}

model appointmentrule {
  AppointmentRuleNum BigInt  @id @default(autoincrement())
  RuleDesc           String? @default("") @db.VarChar(255)
